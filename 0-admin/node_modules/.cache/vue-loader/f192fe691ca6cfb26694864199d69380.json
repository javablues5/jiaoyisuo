{"remainingRequest":"/Users/ying/Downloads/Telegram Desktop/新建文件夹 (2)/echo2.0-admin/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/ying/Downloads/Telegram Desktop/新建文件夹 (2)/echo2.0-admin/src/views/pledge/order/index.vue?vue&type=script&lang=js","dependencies":[{"path":"/Users/ying/Downloads/Telegram Desktop/新建文件夹 (2)/echo2.0-admin/src/views/pledge/order/index.vue","mtime":1698845798738},{"path":"/Users/ying/Downloads/Telegram Desktop/新建文件夹 (2)/echo2.0-admin/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/ying/Downloads/Telegram Desktop/新建文件夹 (2)/echo2.0-admin/node_modules/babel-loader/lib/index.js","mtime":456789000000},{"path":"/Users/ying/Downloads/Telegram Desktop/新建文件夹 (2)/echo2.0-admin/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/ying/Downloads/Telegram Desktop/新建文件夹 (2)/echo2.0-admin/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+QA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/pledge/order","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <SearchFormBox>\r\n      <el-form\r\n        :model=\"queryParams\"\r\n        ref=\"queryForm\"\r\n        size=\"small\"\r\n        :inline=\"true\"\r\n        v-show=\"showSearch\"\r\n        label-width=\"68px\"\r\n      >\r\n        <el-form-item label=\"用户ID\" prop=\"userId\">\r\n          <el-input\r\n            v-model=\"queryParams.userId\"\r\n            clearable\r\n            @keyup.enter.native=\"handleQuery\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"项目id\" prop=\"planId\">\r\n          <el-input\r\n            v-model=\"queryParams.planId\"\r\n            clearable\r\n            @keyup.enter.native=\"handleQuery\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"项目名称\" prop=\"planTitle\">\r\n          <el-input\r\n            v-model=\"queryParams.planTitle\"\r\n            clearable\r\n            @keyup.enter.native=\"handleQuery\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"订单编号\" prop=\"orderNo\">\r\n          <el-input\r\n            v-model=\"queryParams.orderNo\"\r\n            clearable\r\n            @keyup.enter.native=\"handleQuery\"\r\n          />\r\n        </el-form-item>\r\n        <!-- <el-form-item label=\"到期时间\" prop=\"endTime\">\r\n          <el-date-picker\r\n            clearable\r\n            v-model=\"queryParams.endTime\"\r\n            type=\"date\"\r\n            value-format=\"yyyy-MM-dd\"\r\n          >\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"结算时间\" prop=\"settleTime\">\r\n          <el-date-picker\r\n            clearable\r\n            v-model=\"queryParams.settleTime\"\r\n            type=\"date\"\r\n            value-format=\"yyyy-MM-dd\"\r\n          >\r\n          </el-date-picker>\r\n        </el-form-item> -->\r\n\r\n        <el-form-item>\r\n          <el-button\r\n            type=\"primary\"\r\n            icon=\"el-icon-search\"\r\n            size=\"mini\"\r\n            @click=\"handleQuery\"\r\n            >搜索</el-button\r\n          >\r\n          <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetQuery\"\r\n            >重置</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n    </SearchFormBox>\r\n    <TableContentBox>\r\n      <el-row :gutter=\"10\" class=\"mb8\">\r\n        <el-col :span=\"1.5\">\r\n          <el-button\r\n            plain\r\n            size=\"small\"\r\n            type=\"danger\"\r\n            @click=\"handleDelete\"\r\n            v-hasPermi=\"['bussiness:order:remove']\"\r\n            >删除</el-button\r\n          >\r\n        </el-col>\r\n        <right-toolbar\r\n          :showSearch.sync=\"showSearch\"\r\n          @queryTable=\"getList\"\r\n        ></right-toolbar>\r\n      </el-row>\r\n\r\n      <el-table\r\n        height=\"calc(100vh - 400px)\"\r\n        border\r\n        v-loading=\"loading\"\r\n        :data=\"orderList\"\r\n        @selection-change=\"handleSelectionChange\"\r\n      >\r\n        <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\r\n        <el-table-column label=\"ID\" align=\"center\" prop=\"id\" />\r\n        <el-table-column label=\"用户ID\" align=\"center\" prop=\"userId\" />\r\n        <el-table-column label=\"投资金额\" align=\"center\" prop=\"amount\" />\r\n        <el-table-column label=\"投资期限\" align=\"center\" prop=\"days\" />\r\n        <el-table-column label=\"状态\" align=\"center\" prop=\"status\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag\r\n              type=\"primary\"\r\n              size=\"mini\"\r\n              plain\r\n              v-if=\"scope.row.status == 0\"\r\n              >收益</el-tag\r\n            >\r\n            <el-tag\r\n              type=\"info\"\r\n              size=\"mini\"\r\n              plain\r\n              v-else-if=\"scope.row.status == 1\"\r\n              >结算</el-tag\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"项目id\" align=\"center\" prop=\"planId\" />\r\n        <el-table-column label=\"项目名称\" align=\"center\" prop=\"planTitle\" />\r\n        <el-table-column\r\n          label=\"订单编号\"\r\n          align=\"center\"\r\n          prop=\"orderNo\"\r\n          width=\"120\"\r\n          ><template slot-scope=\"scope\">\r\n            <el-link\r\n              :underline=\"false\"\r\n              v-clipboard=\"scope.row.orderNo\"\r\n              v-clipboard:success=\"clipboardSuccess\"\r\n              >{{ scope.row.orderNo }} <i class=\"el-icon-copy-document\"></i>\r\n            </el-link>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"创建时间\"\r\n          align=\"center\"\r\n          prop=\"createTime\"\r\n          width=\"180\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <span>{{ parseTime(scope.row.createTime) }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"到期时间\"\r\n          align=\"center\"\r\n          prop=\"endTime\"\r\n          width=\"180\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <span>{{ parseTime(scope.row.endTime) }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"结算时间\"\r\n          align=\"center\"\r\n          prop=\"updateTime\"\r\n          width=\"180\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <span>{{ parseTime(scope.row.updateTime) }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"累计收益\" align=\"center\" prop=\"accumulaEarn\" />\r\n        <el-table-column label=\"最小利率\" align=\"center\" prop=\"minOdds\" />\r\n        <el-table-column label=\"最大利率\" align=\"center\" prop=\"maxOdds\" />\r\n        <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\"\r\n          align=\"center\"\r\n          class-name=\"small-padding fixed-width\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"small\"\r\n              plain\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.row)\"\r\n              v-hasPermi=\"['bussiness:order:remove']\"\r\n              >删除</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <pagination\r\n        v-show=\"total > 0\"\r\n        :total=\"total\"\r\n        :page.sync=\"queryParams.pageNum\"\r\n        :limit.sync=\"queryParams.pageSize\"\r\n        @pagination=\"getList\"\r\n      />\r\n    </TableContentBox>\r\n    <!-- 添加或修改ming对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item label=\"投资金额\" prop=\"amount\">\r\n          <el-input v-model=\"form.amount\" placeholder=\"请输入投资金额\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"投资期限\" prop=\"days\">\r\n          <el-input v-model=\"form.days\" placeholder=\"请输入投资期限\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"项目id\" prop=\"planId\">\r\n          <el-input v-model=\"form.planId\" placeholder=\"请输入项目id\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"项目名称\" prop=\"planTitle\">\r\n          <el-input v-model=\"form.planTitle\" placeholder=\"请输入项目名称\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"订单编号\" prop=\"orderNo\">\r\n          <el-input v-model=\"form.orderNo\" placeholder=\"请输入订单编号\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"到期时间\" prop=\"endTime\">\r\n          <el-date-picker\r\n            clearable\r\n            v-model=\"form.endTime\"\r\n            type=\"date\"\r\n            value-format=\"yyyy-MM-dd\"\r\n            placeholder=\"请选择到期时间\"\r\n          >\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"结算时间\" prop=\"settleTime\">\r\n          <el-date-picker\r\n            clearable\r\n            v-model=\"form.settleTime\"\r\n            type=\"date\"\r\n            value-format=\"yyyy-MM-dd\"\r\n            placeholder=\"请选择结算时间\"\r\n          >\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"累计收益\" prop=\"accumulaEarn\">\r\n          <el-input v-model=\"form.accumulaEarn\" placeholder=\"请输入累计收益\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"最小利率\" prop=\"minOdds\">\r\n          <el-input v-model=\"form.minOdds\" placeholder=\"请输入最小利率\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"最大利率\" prop=\"maxOdds\">\r\n          <el-input v-model=\"form.maxOdds\" placeholder=\"请输入最大利率\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"后台用户id\" prop=\"adminUserIds\">\r\n          <el-input\r\n            v-model=\"form.adminUserIds\"\r\n            placeholder=\"请输入后台用户id\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"${comment}\" prop=\"collectionOrder\">\r\n          <el-input\r\n            v-model=\"form.collectionOrder\"\r\n            placeholder=\"请输入${comment}\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"${comment}\" prop=\"userId\">\r\n          <el-input v-model=\"form.userId\" placeholder=\"请输入${comment}\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"${comment}\" prop=\"orderAmount\">\r\n          <el-input v-model=\"form.orderAmount\" placeholder=\"请输入${comment}\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  listOrder,\r\n  getOrder,\r\n  delOrder,\r\n  addOrder,\r\n  updateOrder,\r\n} from \"@/api/bussiness/order\";\r\n\r\nexport default {\r\n  name: \"Order\",\r\n  data() {\r\n    return {\r\n      // 遮罩层\r\n      loading: true,\r\n      // 选中数组\r\n      ids: [],\r\n      // 非单个禁用\r\n      single: true,\r\n      // 非多个禁用\r\n      multiple: true,\r\n      // 显示搜索条件\r\n      showSearch: true,\r\n      // 总条数\r\n      total: 0,\r\n      // ming表格数据\r\n      orderList: [],\r\n      // 弹出层标题\r\n      title: \"\",\r\n      // 是否显示弹出层\r\n      open: false,\r\n      // 查询参数\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        amount: null,\r\n        days: null,\r\n        status: null,\r\n        planId: null,\r\n        planTitle: null,\r\n        orderNo: null,\r\n        endTime: null,\r\n        settleTime: null,\r\n        accumulaEarn: null,\r\n        minOdds: null,\r\n        maxOdds: null,\r\n        adminUserIds: null,\r\n        collectionOrder: null,\r\n        userId: null,\r\n        orderAmount: null,\r\n      },\r\n      // 表单参数\r\n      form: {},\r\n      // 表单校验\r\n      rules: {\r\n        amount: [\r\n          { required: true, message: \"投资金额不能为空\", trigger: \"blur\" },\r\n        ],\r\n        days: [\r\n          { required: true, message: \"投资期限不能为空\", trigger: \"blur\" },\r\n        ],\r\n        planId: [\r\n          { required: true, message: \"项目id不能为空\", trigger: \"blur\" },\r\n        ],\r\n        orderNo: [\r\n          { required: true, message: \"订单编号不能为空\", trigger: \"blur\" },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n    /** 复制代码成功 */\r\n    clipboardSuccess() {\r\n      this.$modal.msgSuccess(\"复制成功\");\r\n    },\r\n    /** 查询ming列表 */\r\n    getList() {\r\n      this.loading = true;\r\n      listOrder(this.queryParams).then((response) => {\r\n        this.orderList = response.rows;\r\n        this.total = response.total;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.open = false;\r\n      this.reset();\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        id: null,\r\n        amount: null,\r\n        days: null,\r\n        status: null,\r\n        planId: null,\r\n        planTitle: null,\r\n        orderNo: null,\r\n        createTime: null,\r\n        endTime: null,\r\n        settleTime: null,\r\n        accumulaEarn: null,\r\n        updateTime: null,\r\n        minOdds: null,\r\n        maxOdds: null,\r\n        adminUserIds: null,\r\n        collectionOrder: null,\r\n        userId: null,\r\n        orderAmount: null,\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n    /** 搜索按钮操作 */\r\n    handleQuery() {\r\n      this.queryParams.pageNum = 1;\r\n      this.getList();\r\n    },\r\n    /** 重置按钮操作 */\r\n    resetQuery() {\r\n      this.resetForm(\"queryForm\");\r\n      this.handleQuery();\r\n    },\r\n    // 多选框选中数据\r\n    handleSelectionChange(selection) {\r\n      this.ids = selection.map((item) => item.id);\r\n      this.single = selection.length !== 1;\r\n      this.multiple = !selection.length;\r\n    },\r\n    /** 新增按钮操作 */\r\n    handleAdd() {\r\n      this.reset();\r\n      this.open = true;\r\n      this.title = \"添加ming\";\r\n    },\r\n    /** 修改按钮操作 */\r\n    handleUpdate(row) {\r\n      this.reset();\r\n      const id = row.id || this.ids;\r\n      getOrder(id).then((response) => {\r\n        this.form = response.data;\r\n        this.open = true;\r\n        this.title = \"修改ming\";\r\n      });\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate((valid) => {\r\n        if (valid) {\r\n          if (this.form.id != null) {\r\n            updateOrder(this.form).then((response) => {\r\n              this.$modal.msgSuccess(\"修改成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          } else {\r\n            addOrder(this.form).then((response) => {\r\n              this.$modal.msgSuccess(\"新增成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    /** 删除按钮操作 */\r\n    handleDelete(row) {\r\n      const ids = row.id || this.ids;\r\n      this.$modal\r\n        .confirm('是否确认删除ming编号为\"' + ids + '\"的数据项？')\r\n        .then(function () {\r\n          return delOrder(ids);\r\n        })\r\n        .then(() => {\r\n          this.getList();\r\n          this.$modal.msgSuccess(\"删除成功\");\r\n        })\r\n        .catch(() => {});\r\n    },\r\n    /** 导出按钮操作 */\r\n    handleExport() {\r\n      this.download(\r\n        \"bussiness/order/export\",\r\n        {\r\n          ...this.queryParams,\r\n        },\r\n        `order_${new Date().getTime()}.xlsx`\r\n      );\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}