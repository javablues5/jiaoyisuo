{"remainingRequest":"/Users/atk/Desktop/交易所9.27/0-admin/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/atk/Desktop/交易所9.27/0-admin/src/views/bussiness/user/components/DMoney.vue?vue&type=script&lang=js","dependencies":[{"path":"/Users/atk/Desktop/交易所9.27/0-admin/src/views/bussiness/user/components/DMoney.vue","mtime":1698845798000},{"path":"/Users/atk/Desktop/交易所9.27/0-admin/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/atk/Desktop/交易所9.27/0-admin/node_modules/babel-loader/lib/index.js","mtime":456789000000},{"path":"/Users/atk/Desktop/交易所9.27/0-admin/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/atk/Desktop/交易所9.27/0-admin/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["DMoney.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoFA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"DMoney.vue","sourceRoot":"src/views/bussiness/user/components","sourcesContent":["<!-- 下分 -->\r\n<template>\r\n  <el-dialog\r\n    title=\"人工下分\"\r\n    :visible.sync=\"visible\"\r\n    width=\"500px\"\r\n    append-to-body\r\n    @close=\"cancel\"\r\n    \r\n  >\r\n    <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n      <el-form-item label=\"用户ID\">\r\n        <el-input disabled :value=\"user.userId\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"账号\">\r\n        <el-input disabled :value=\"user.loginName\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"类型\" prop=\"giveType\">\r\n        <el-select\r\n          v-model=\"form.giveType\"\r\n          placeholder=\"请选择上下分\"\r\n          style=\"width: 100%\"\r\n        >\r\n          <el-option label=\"上分\" value=\"0\"></el-option>\r\n          <el-option label=\"下分\" value=\"1\"></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"资产类型\" prop=\"type\">\r\n        <el-select\r\n          v-model=\"form.type\"\r\n          placeholder=\"请选择资产类型\"\r\n          clearable\r\n          style=\"width: 100%\"\r\n        >\r\n          <el-option\r\n            v-for=\"dict in dict.type.t_asset_type\"\r\n            :key=\"dict.value\"\r\n            :label=\"dict.label\"\r\n            :value=\"dict.value\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"币种\" prop=\"symbol\">\r\n        <el-select\r\n          v-model=\"form.symbol\"\r\n          placeholder=\"请选择币种\"\r\n          clearable\r\n          style=\"width: 100%\"\r\n        >\r\n          <el-option\r\n            v-for=\"dict in showAssetList\"\r\n            :key=\"dict.symbol\"\r\n            :label=\"dict.symbol\"\r\n            :value=\"dict.symbol\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"现有金额\">\r\n        <el-input disabled :value=\"hasAmount\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"金额\" prop=\"amount\">\r\n        <el-input\r\n          v-model=\"form.amount\"\r\n          type=\"number\"\r\n          placeholder=\"请输入金额\"\r\n        />\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"备注\" prop=\"remark\">\r\n        <el-input\r\n          v-model=\"form.remark\"\r\n          type=\"textarea\"\r\n          placeholder=\"请输入备注\"\r\n        />\r\n      </el-form-item>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      <el-button @click=\"cancel\">取 消</el-button>\r\n    </div>\r\n  </el-dialog>\r\n</template>\r\n<script>\r\nimport { getAssetList, subAmountApi } from \"@/api/bussiness/user.js\";\r\nexport default {\r\n  dicts: [\"t_asset_type\"],\r\n  props: {\r\n    value: {\r\n      type: Boolean,\r\n      required: true,\r\n    },\r\n    user: {\r\n      type: Object,\r\n      default: () => {},\r\n    },\r\n  },\r\n\r\n  watch: {\r\n    user: {\r\n      handler() {\r\n        this.getAssetList();\r\n      },\r\n      deep: true,\r\n      immediate: true,\r\n    },\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      // 资产列表\r\n      assetList: [],\r\n      // 表单参数\r\n      form: {},\r\n      // 表单校验\r\n      rules: {\r\n        amount: [{ required: true, message: \"金额不能为空\", trigger: \"blur\" }],\r\n        type: [\r\n          { required: true, message: \"资产类型不能为空\", trigger: \"blur\" },\r\n        ],\r\n        giveType: [\r\n          { required: true, message: \"上下分类型不能为空\", trigger: \"blur\" },\r\n        ],\r\n        symbol: [{ required: true, message: \"币种不能为空\", trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    visible: {\r\n      get() {\r\n        return this.value;\r\n      },\r\n      set(val) {},\r\n    },\r\n    hasAmount() {\r\n      let tempObj = this.showAssetList.find(\r\n        (elem) => elem.symbol == this.form.symbol\r\n      );\r\n      return tempObj?.amout || 0;\r\n    },\r\n    /**\r\n     * 资产列表\r\n     */\r\n    showAssetList() {\r\n      // return this.assetList.filter((elem) => elem.type == this.form.type);\r\n      let list = this.assetList.filter((elem) => elem.type == this.form.type);\r\n      let reversedNumbers = [];\r\n      for (let i = list.length - 1; i >= 0; i--) {\r\n        reversedNumbers.push(list[i]);\r\n      }\r\n      return reversedNumbers;\r\n    },\r\n  },\r\n  methods: {\r\n    async getAssetList() {\r\n      if (this.user?.userId) {\r\n        this.reset();\r\n        this.form.type = this.dict.type.t_asset_type[0]?.value;\r\n        this.form.userId = this.user.userId;\r\n\r\n        const res = await getAssetList({\r\n          userId: this.user.userId,\r\n          pageNum: 1,\r\n          pageSize: 1000,\r\n        });\r\n        if (res.code == 200) {\r\n          this.assetList = res.rows;\r\n          this.form.symbol = this.showAssetList[0]?.symbol || \"\";\r\n        }\r\n      }\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        userId: null,\r\n        type: null,\r\n        amount: null,\r\n        symbol: null,\r\n        remark: null,\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate((valid) => {\r\n        if (valid) {\r\n          this.form.userId = this.user.userId;\r\n          subAmountApi(this.form)\r\n            .then((res) => {\r\n              if (res.code == 200) {\r\n                this.$modal.msgSuccess(res.msg);\r\n              } else {\r\n                this.$modal.msgError(res.msg);\r\n              }\r\n              this.getAssetList();\r\n            })\r\n            .finally(() => {\r\n              this.getAssetList();\r\n              this.cancel();\r\n            });\r\n        }\r\n      });\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.$emit(\"input\", false);\r\n      this.reset();\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}