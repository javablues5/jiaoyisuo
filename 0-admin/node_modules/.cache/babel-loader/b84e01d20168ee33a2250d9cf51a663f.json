\">\r\n          <el-button\r\n            type=\"primary\"\r\n            size=\"mini\"\r\n            @click=\"handleOneAdd\"\r\n            v-hasPermi=\"['bussiness:currency:symbol:add']\"\r\n            >一键新增</el-button\r\n          >\r\n        </el-col> -->\r\n        <el-col :span=\"1.5\">\r\n          <el-button\r\n            type=\"primary\"\r\n            plain\r\n            icon=\"el-icon-plus\"\r\n            size=\"mini\"\r\n            @click=\"handleAdd\"\r\n            v-hasPermi=\"['bussiness:currency:symbol:add']\"\r\n            >新增</el-button\r\n          >\r\n        </el-col>\r\n        <!-- <el-col :span=\"1.5\">\r\n          <el-button\r\n            type=\"primary\"\r\n            plain\r\n            size=\"mini\"\r\n            :disabled=\"single\"\r\n            @click=\"handleUpdate\"\r\n            v-hasPermi=\"['bussiness:currency:symbol:edit']\"\r\n            >修改</el-button\r\n          >\r\n        </el-col> -->\r\n        <el-col :span=\"1.5\">\r\n          <el-button\r\n            type=\"danger\"\r\n            plain\r\n            size=\"mini\"\r\n            :disabled=\"multiple\"\r\n            @click=\"handleDelete\"\r\n            v-hasPermi=\"['bussiness:currency:symbol:remove']\"\r\n            >删除</el-button\r\n          >\r\n        </el-col>\r\n        <!-- <el-col :span=\"1.5\">\r\n          <el-button\r\n            type=\"warning\"\r\n            plain\r\n            icon=\"el-icon-download\"\r\n            size=\"mini\"\r\n            :disabled=\"multiple\"\r\n            @click=\"handleExport\"\r\n            v-hasPermi=\"['bussiness:currency:export']\"\r\n            >导出</el-button\r\n          >\r\n        </el-col> -->\r\n        <right-toolbar\r\n          :showSearch.sync=\"showSearch\"\r\n          @queryTable=\"getList\"\r\n        ></right-toolbar>\r\n      </el-row>\r\n\r\n      <el-table\r\n        height=\"calc(100vh - 400px)\"\r\n        border\r\n        v-loading=\"loading\"\r\n        :data=\"currencyList\"\r\n        @selection-change=\"handleSelectionChange\"\r\n      >\r\n        <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\r\n        <el-table-column type=\"index\" width=\"50\" align=\"center\" />\r\n        <!-- <el-table-column label=\"id\" align=\"center\" prop=\"id\" /> -->\r\n        <el-table-column label=\"图标\" align=\"center\" prop=\"logo\">\r\n          <template slot-scope=\"scope\">\r\n            <img :src=\"scope.row.logo\" style=\"width: 30px\" />\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"交易对\"\r\n          align=\"center\"\r\n          prop=\"symbol\"\r\n          width=\"120\"\r\n        />\r\n        <el-table-column\r\n          label=\"前端展示交易对\"\r\n          align=\"center\"\r\n          prop=\"showSymbol\"\r\n          width=\"120\"\r\n        />\r\n\r\n        <el-table-column label=\"交易 / 结算币种\" width=\"120\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <div>\r\n              {{ scope.row.coin }}<span> / </span>{{ scope.row.baseCoin }}\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"手续费(%)\" align=\"center\" prop=\"feeRate\" />\r\n        <el-table-column label=\"最小 / 最大下单量\" width=\"90\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <div>\r\n              {{ scope.row.orderMin }}<span> / </span>{{ scope.row.orderMax }}\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"最低卖出量\"\r\n          width=\"90\"\r\n          align=\"center\"\r\n          prop=\"minSell\"\r\n        >\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"启用禁用\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag\r\n              type=\"primary\"\r\n              size=\"mini\"\r\n              plain\r\n              v-if=\"scope.row.enable == 1\"\r\n              >启用</el-tag\r\n            >\r\n            <el-tag\r\n              type=\"info\"\r\n              size=\"mini\"\r\n              plain\r\n              v-else-if=\"scope.row.enable == 2\"\r\n              >禁用</el-tag\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n        <!-- <el-table-column label=\"显示\" align=\"center\" prop=\"isShow\" width=\"60\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag type=\"danger\" size=\"mini\" plain v-if=\"scope.row.isShow == 1\"\r\n              >显示</el-tag\r\n            >\r\n            <el-tag\r\n              type=\"info\"\r\n              size=\"mini\"\r\n              plain\r\n              v-else-if=\"scope.row.isShow == 2\"\r\n              >隐藏</el-tag\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"可交易\" align=\"center\" width=\"60\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag\r\n              type=\"success\"\r\n              size=\"mini\"\r\n              plain\r\n              v-if=\"scope.row.isDeal == 1\"\r\n              >是\r\n            </el-tag>\r\n            <el-tag\r\n              type=\"info\"\r\n              size=\"mini\"\r\n              plain\r\n              v-else-if=\"scope.row.isDeal == 2\"\r\n              >否</el-tag\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"市价买\" align=\"center\" width=\"60\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag\r\n              type=\"success\"\r\n              size=\"mini\"\r\n              plain\r\n              v-if=\"scope.row.marketBuy == 1\"\r\n              >是\r\n            </el-tag>\r\n            <el-tag\r\n              type=\"info\"\r\n              size=\"mini\"\r\n              plain\r\n              v-else-if=\"scope.row.marketBuy == 2\"\r\n              >否</el-tag\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"市价卖\" align=\"center\" width=\"60\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag\r\n              type=\"success\"\r\n              size=\"mini\"\r\n              plain\r\n              v-if=\"scope.row.marketSell == 1\"\r\n              >是\r\n            </el-tag>\r\n            <el-tag\r\n              type=\"info\"\r\n              size=\"mini\"\r\n              plain\r\n              v-else-if=\"scope.row.marketSell == 2\"\r\n              >否</el-tag\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"限价买\" align=\"center\" width=\"60\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag\r\n              type=\"success\"\r\n              size=\"mini\"\r\n              plain\r\n              v-if=\"scope.row.limitedBuy == 1\"\r\n              >是\r\n            </el-tag>\r\n            <el-tag\r\n              type=\"info\"\r\n              size=\"mini\"\r\n              plain\r\n              v-else-if=\"scope.row.limitedBuy == 2\"\r\n              >否</el-tag\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"限价卖\" align=\"center\" width=\"60\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag\r\n              type=\"success\"\r\n              size=\"mini\"\r\n              plain\r\n              v-if=\"scope.row.limitedSell == 1\"\r\n              >是\r\n            </el-tag>\r\n            <el-tag\r\n              type=\"info\"\r\n              size=\"mini\"\r\n              plain\r\n              v-else-if=\"scope.row.limitedSell == 2\"\r\n              >否</el-tag\r\n            >\r\n          </template>\r\n        </el-table-column> -->\r\n\r\n        <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\"\r\n          width=\"150\"\r\n          align=\"center\"\r\n          class-name=\"small-padding fixed-width\"\r\n        >\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"small\"\r\n              type=\"pramiary\"\r\n              plain\r\n              @click=\"handleUpdate(scope.row)\"\r\n              v-hasPermi=\"['bussiness:currency:symbol:query']\"\r\n              >修改</el-button\r\n            >\r\n            <el-button\r\n              size=\"small\"\r\n              type=\"danger\"\r\n              plain\r\n              @click=\"handleDelete(scope.row)\"\r\n              v-hasPermi=\"['bussiness:currency:symbol:remove']\"\r\n              >删除</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <pagination\r\n        v-show=\"total > 0\"\r\n        :total=\"total\"\r\n        :page.sync=\"queryParams.pageNum\"\r\n        :limit.sync=\"queryParams.pageSize\"\r\n        @pagination=\"getList\"\r\n      />\r\n    </TableContentBox>\r\n    <!-- 添加或修改币币交易币种配置对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"700px\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"150px\">\r\n        <div class=\"select\">\r\n          <el-form-item label=\"展示交易对\" prop=\"showSymbol\">\r\n            <el-input v-model=\"form.showSymbol\" />\r\n          </el-form-item>\r\n          <el-form-item label=\"币种\" prop=\"currentId\">\r\n            <el-select\r\n              v-model=\"form.currentId\"\r\n              filterable\r\n              clearable\r\n              style=\"width: 100%\"\r\n              @change=\"changeCoin\"\r\n            >\r\n              <el-option\r\n                v-for=\"coin in coinList\"\r\n                :key=\"coin.id\"\r\n                :label=\"coin.label\"\r\n                :value=\"coin.id\"\r\n              />\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"手续费率\" prop=\"feeRate\">\r\n            <el-input v-model=\"form.feeRate\" />\r\n          </el-form-item>\r\n          <el-form-item label=\"最小下单量\" prop=\"orderMin\">\r\n            <el-input type=\"number\" v-model=\"form.orderMin\" />\r\n          </el-form-item>\r\n          <el-form-item label=\"最大下单量\" prop=\"orderMax\">\r\n            <el-input type=\"number\" v-model=\"form.orderMax\" />\r\n          </el-form-item>\r\n          <el-form-item label=\"最低卖出量\" prop=\"minSell\">\r\n            <el-input v-model=\"form.minSell\" />\r\n          </el-form-item>\r\n        </div>\r\n        <!-- 1展示 2不展示 -->\r\n        <el-form-item label=\"启用\" prop=\"enable\">\r\n          <el-switch\r\n            v-model=\"form.enable\"\r\n            :active-value=\"'1'\"\r\n            :inactive-value=\"'2'\"\r\n          ></el-switch>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button\r\n          type=\"primary\"\r\n          @click=\"submitForm\"\r\n          v-hasPermi=\"['bussiness:currency:symbol:edit']\"\r\n          >确 定</el-button\r\n        >\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <el-dialog\r\n      :title=\"title\"\r\n      :visible.sync=\"oneOpen\"\r\n      width=\"800px\"\r\n      append-to-body\r\n    >\r\n      <el-form label-width=\"80px\">\r\n        <el-row class=\"row\">\r\n          <el-col :span=\"20\">\r\n            <el-table\r\n              v-loading=\"loading\"\r\n              :data=\"coinList\"\r\n              @selection-change=\"bindList\"\r\n            >\r\n              <el-table-column type=\"selection\" width=\"50\" align=\"center\" />\r\n              <el-table-column label=\"图标\" align=\"center\" prop=\"logo\">\r\n                <template slot-scope=\"scope\">\r\n                  <img :src=\"scope.row.logo\" style=\"width: 30px\" />\r\n                </template>\r\n              </el-table-column>\r\n              <el-table-column\r\n                label=\"币种名称\"\r\n                align=\"center\"\r\n                key=\"symbol\"\r\n                prop=\"symbol\"\r\n              />\r\n              <!-- v-if=\"columns[0].visible\" -->\r\n            </el-table>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <!-- <pagination\r\n        v-show=\"adminTotal > 0\"\r\n        :total=\"adminTotal\"\r\n        :page.sync=\"queryParams1.pageNum\"\r\n        :limit.sync=\"queryParams1.pageSize\"\r\n        @pagination=\"getAdminUserList\"\r\n      /> -->\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button\r\n          type=\"primary\"\r\n          @click=\"submitOneForm\"\r\n          v-hasPermi=\"['bussiness:currency:symbol:add']\"\r\n          >确 定</el-button\r\n        >\r\n        <el-button @click=\"oneCancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  listConfiguration,\r\n  getCurrency,\r\n  delCurrency,\r\n  addCurrency,\r\n  updateCurrency,\r\n  addOneCurrency,\r\n  getCoinList,\r\n} from \"@/api/bussiness/currency\";\r\n\r\nexport default {\r\n  name: \"Currency\",\r\n  data() {\r\n    return {\r\n      currentMarket: \"\",\r\n      coinList: [],\r\n      addCoinForm: { symbols: undefined },\r\n      oneOpen: false,\r\n      // 遮罩层\r\n      loading: true,\r\n      // 选中数组\r\n      ids: [],\r\n      // 非单个禁用\r\n      single: true,\r\n      // 非多个禁用\r\n      multiple: true,\r\n      // 显示搜索条件\r\n      showSearch: true,\r\n      // 总条数\r\n      total: 0,\r\n      // 币币交易币种配置表格数据\r\n      currencyList: [],\r\n      // 弹出层标题\r\n      title: \"\",\r\n      // 是否显示弹出层\r\n      open: false,\r\n      // 查询参数\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        symbol: null,\r\n        coin: null,\r\n        baseCoin: null,\r\n        feeRate: null,\r\n        coinPrecision: null,\r\n        basePrecision: null,\r\n        sellMin: null,\r\n        buyMax: null,\r\n        o