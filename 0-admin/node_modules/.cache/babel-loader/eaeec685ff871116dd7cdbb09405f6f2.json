\r\n    };\r\n  },\r\n  computed: {\r\n    visible: {\r\n      get() {\r\n        return this.value;\r\n      },\r\n      set(val) {},\r\n    },\r\n    hasAmount() {\r\n      let tempObj = this.showAssetList.find(\r\n        (elem) => elem.symbol == this.form.symbol\r\n      );\r\n      return tempObj?.amout || 0;\r\n    },\r\n    /**\r\n     * 资产列表\r\n     */\r\n    showAssetList() {\r\n      // return this.assetList.filter((elem) => elem.type == this.form.type);\r\n      let list = this.assetList.filter((elem) => elem.type == this.form.type);\r\n      let reversedNumbers = [];\r\n      for (let i = list.length - 1; i >= 0; i--) {\r\n        reversedNumbers.push(list[i]);\r\n      }\r\n      return reversedNumbers;\r\n    },\r\n  },\r\n  mounted() {},\r\n  methods: {\r\n    async getAssetList() {\r\n      if (this.user?.userId) {\r\n        this.reset();\r\n        this.form.type = this.dict.type.t_asset_type[0]?.value;\r\n        this.form.userId = this.user.userId;\r\n\r\n        const res = await getAssetList({\r\n          userId: this.user.userId,\r\n          pageNum: 1,\r\n          pageSize: 1000,\r\n        });\r\n        if (res.code == 200) {\r\n          this.assetList = res.rows;\r\n          this.form.symbol = this.showAssetList[0]?.symbol || \"\";\r\n        }\r\n      }\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        userId: null,\r\n        type: null,\r\n        amount: null,\r\n        symbol: null,\r\n        remark: null,\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate((valid) => {\r\n        if (valid) {\r\n          this.form.userId = this.user.userId;\r\n          sendBousApi(this.form)\r\n            .then((res) => {\r\n              if (res.code == 200) {\r\n                this.$modal.msgSuccess(res.msg);\r\n                this.$emit(\"getDataList\");\r\n              } else {\r\n                this.$modal.msgError(res.msg);\r\n              }\r\n            })\r\n            .finally(() => {\r\n              this.cancel();\r\n            });\r\n        }\r\n      });\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.$emit(\"input\", false);\r\n      this.reset();\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";;;;;;;;;;;;;AAmFA,IAAAA,KAAA,GAAAC,OAAA;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;kCACA;EACAC,KAAA;EACAC,KAAA;IACAC,KAAA;MACAC,IAAA,EAAAC,OAAA;MACAC,QAAA;IACA;IACAC,IAAA;MACAH,IAAA,EAAAI,MAAA;MACAC,OAAA,WAAAC,SAAA;IACA;EACA;EAEAC,KAAA;IACAJ,IAAA;MACAK,OAAA,WAAAA,QAAA;QACA,KAAAC,YAAA;MACA;MACAC,IAAA;MACAC,SAAA;IACA;EACA;EAEAC,IAAA,WAAAA,KAAA;IACA;MACA;MACAC,SAAA;MACA;MACAC,IAAA;MACA;MACAC,KAAA;QACAC,MAAA;UAAAd,QAAA;UAAAe,OAAA;UAAAC,OAAA;QAAA;QACAlB,IAAA,GACA;UAAAE,QAAA;UAAAe,OAAA;UAAAC,OAAA;QAAA,EACA;QACAC,QAAA,GACA;UACAjB,QAAA;UACAe,OAAA;UACAC,OAAA;QACA,EACA;QACAE,MAAA;UAAAlB,QAAA;UAAAe,OAAA;UAAAC,OAAA;QAAA;MACA;IACA;EACA;EACAG,QAAA;IACAC,OAAA;MACAC,GAAA,WAAAA,IAAA;QACA,YAAAxB,KAAA;MACA;MACAyB,GAAA,WAAAA,IAAAC,GAAA;IACA;IACAC,SAAA,WAAAA,UAAA;MAAA,IAAAC,KAAA;MACA,IAAAC,OAAA,QAAAC,aAAA,CAAAC,IAAA,CACA,UAAAC,IAAA;QAAA,OAAAA,IAAA,CAAAX,MAAA,IAAAO,KAAA,CAAAb,IAAA,CAAAM,MAAA;MAAA,CACA;MACA,QAAAQ,OAAA,aAAAA,OAAA,uBAAAA,OAAA,CAAAI,KAAA;IACA;IACA;AACA;AACA;IACAH,aAAA,WAAAA,cAAA;MAAA,IAAAI,MAAA;MACA;MACA,IAAAC,IAAA,QAAArB,SAAA,CAAAsB,MAAA,WAAAJ,IAAA;QAAA,OAAAA,IAAA,CAAA/B,IAAA,IAAAiC,MAAA,CAAAnB,IAAA,CAAAd,IAAA;MAAA;MACA,IAAAoC,eAAA;MACA,SAAAC,CAAA,GAAAH,IAAA,CAAAI,MAAA,MAAAD,CAAA,OAAAA,CAAA;QACAD,eAAA,CAAAG,IAAA,CAAAL,IAAA,CAAAG,CAAA;MACA;MACA,OAAAD,eAAA;IACA;EACA;EACAI,OAAA,WAAAA,QAAA;EACAC,OAAA;IACAhC,YAAA,WAAAA,aAAA;MAAA,IAAAiC,MAAA;MAAA,WAAAC,kBAAA,CAAAtC,OAAA,oBAAAuC,oBAAA,CAAAvC,OAAA,IAAAwC,IAAA,UAAAC,QAAA;QAAA,IAAAC,WAAA;QAAA,IAAAC,qBAAA,EAAAC,GAAA,EAAAC,qBAAA;QAAA,WAAAN,oBAAA,CAAAvC,OAAA,IAAA8C,IAAA,UAAAC,SAAAC,QAAA;UAAA,kBAAAA,QAAA,CAAAC,IAAA,GAAAD,QAAA,CAAAE,IAAA;YAAA;cAAA,OAAAR,WAAA,GACAL,MAAA,CAAAvC,IAAA,cAAA4C,WAAA,eA